@import '../../common/common.wxss';
page{
  background: #f5f5f5!important;
}
.cart-box{
    padding-bottom: 100rpx;
}
.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 235rpx;
    height: 95rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 40rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 85rpx;
    width: 105rpx;
    height: 105rpx;
}
.cart-list .cart-pro-name{
    display: inline-block;
    /* width: 380rpx; */
    width: 365rpx;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
}
.cart-list .cart-pro-price{
    display: inline-block;
    float: right;
    height: 50rpx;
    line-height: 50rpx;
    color: #F53A3A;
}
.cart-list .cart-count-box{
    position: absolute;
    /* left: 285; */
    /* bottom: 20rpx; */
    width: 250rpx;
    height: 40rpx;
}
.cart-list .cart-count-box text{
    display: inline-block;
    line-height: 40rpx;
    text-align: center;
}
.cart-count-down,.cart-count-add{
    font-size: 44rpx;
    width: 50rpx;
    height: 100%;
}
.cart-count-num{
    width: 150rpx;
}
.cart-del{
    position: absolute;
    right: 20rpx;
    /* bottom: 20rpx; */
    width: 80rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 28rpx;
    top: 80rpx;
}
.cart-footer{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    /* padding:0 100rpx 0 80rpx; */
    /* padding:0 0 0 80rpx; */
    box-sizing: border-box;
     background:  #F3C313; 
    /* background: #fff; */
    color: #fff;
}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon image,.order-icon navigator{
  display: block;
  width: 45rpx;
  height: 45rpx;
}
